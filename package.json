{
  "name": "Microbium",
  "version": "0.11.0",
  "author": "Jay Weeks <jay.patrick.weeks@gmail.com>",
  "description": "Microbiome drawing and simulating toy.",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "dev:embed": "cross-env BUILD_TARGET=embed SCENE_FIXTURE=002 node .electron-vue/dev-runner.js",
    "build": "sh ./.electron-vue/build.sh",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node ./.electron-vue/build-runner.js",
    "build:embed": "cross-env BUILD_TARGET=embed node ./.electron-vue/build-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "build": {
    "productName": "Microbium",
    "copyright": "Copyright Â© 2017-2018 Jay Weeks",
    "appId": "com.jayweeks.microbium",
    "fileAssociations": [
      {
        "ext": "mcrbm",
        "name": "Microbium Scene",
        "role": "Editor",
        "isPackage": false
      }
    ],
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/microbium.icns"
    }
  },
  "dependencies": {
    "@jpweeks/electron-recorder": "^1.1.0",
    "cardinal-spline": "^0.0.1",
    "colr": "^1.2.2",
    "electron-log": "^2.2.14",
    "electron-store": "^2.0.0",
    "events": "^2.0.0",
    "fs-extra": "^5.0.0",
    "gl-matrix": "^2.4.0",
    "glsl-blend": "^1.0.3",
    "glsl-fast-gaussian-blur": "^1.0.2",
    "glsl-hash-blur": "^1.0.3",
    "glsl-inject-defines": "^1.0.3",
    "glsl-noise": "^0.0.0",
    "glsl-random": "^0.0.5",
    "lodash.merge": "^4.6.1",
    "lodash.template": "^4.4.0",
    "particulate": "^0.3.3",
    "pngjs": "^3.3.3",
    "regl": "^1.3.1",
    "regl-line-builder": "^0.4.2",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-range-slider": "^0.2.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.3.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.0.4",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.2",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^2.0.9",
    "mocha": "^5.0.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "require-dir": "^1.0.0",
    "sass-loader": "^6.0.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^4.0.2",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  }
}
